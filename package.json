{
  "name": "@wspro/di",
  "version": "1.0.2",
  "description": "Symple DI",
  "author": "https://github.com/wsp-repo/",
  "license": "ISC",
  "keywords": [
    "di",
    "injectable",
    "inject"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/wsp-repo/di.git"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.{ts,js}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --quiet --fix",
    "lint:warns": "eslint 'src/**/*.ts' --max-warnings 0",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "reflect-metadata": "0.2.2"
  },
  "devDependencies": {
    "@types/node": "22.1.0",
    "@types/reflect-metadata": "0.1.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@wspro/linter": "1.0.8",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.5.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-typescript-sort": "0.1.11",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "lint-staged": "15.2.8",
    "prettier": "3.2.5",
    "rimraf": "6.0.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --max-warnings 0",
      "eslint --quiet --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --allow-empty"
    }
  },
  "prettier": "@wspro/linter/prettier"
}
